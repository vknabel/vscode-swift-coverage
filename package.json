{
  "name": "swift-coverage",
  "displayName": "Swift Coverage",
  "description": "Displays code coverage reports of Swift Package Manager.",
  "version": "1.0.0",
  "license": "MIT",
  "publisher": "vknabel",
  "author": {
    "name": "Valentin Knabel",
    "email": "dev@vknabel.com",
    "url": "https://github.com/vknabel"
  },
  "galleryBanner": {
    "color": "#30d158",
    "theme": "light"
  },
  "keywords": [
    "swift",
    "coverage",
    "cov",
    "swiftpm",
    "guters"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/vknabel/vscode-swift-coverage"
  },
  "engines": {
    "vscode": "^1.48.0"
  },
  "icon": "./assets/icon.png",
  "categories": [
    "Programming Languages",
    "Visualization"
  ],
  "activationEvents": [
    "workspaceContains:**/.build/*/debug/codecov/*.json",
    "workspaceContains:Package.swift"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [],
    "configuration": {
      "title": "swift-coverage",
      "properties": {
        "swift-coverage.coverageFilePattern": {
          "type": "string",
          "default": "**/.build/*/debug/codecov/*.json",
          "description": ""
        },
        "swift-coverage.highlightsCoveredText": {
          "type": "boolean",
          "default": false,
          "description": ""
        },
        "swift-coverage.highlightsNotCoveredText": {
          "type": "boolean",
          "default": true,
          "description": ""
        },
        "swift-coverage.coveredTextBackground.dark": {
          "type": "string",
          "default": "rgba(48, 209, 88, 0.25)",
          "description": ""
        },
        "swift-coverage.coveredTextBackground.light": {
          "type": "string",
          "default": "rgba(52, 199, 89, 0.25)",
          "description": ""
        },
        "swift-coverage.notCoveredTextBackground.dark": {
          "type": "string",
          "default": "rgba(255, 69, 58, 0.25)",
          "description": ""
        },
        "swift-coverage.notCoveredTextBackground.light": {
          "type": "string",
          "default": "rgba(255, 59, 48, 0.25)",
          "description": ""
        },
        "swift-coverage.highlightsCoveredGutter": {
          "type": "boolean",
          "default": true,
          "description": ""
        },
        "swift-coverage.highlightsNotCoveredGutter": {
          "type": "boolean",
          "default": true,
          "description": ""
        },
        "swift-coverage.highlightsMixedGutter": {
          "type": "boolean",
          "default": true,
          "description": ""
        },
        "swift-coverage.coveredGutterIcon.dark": {
          "type": "string",
          "default": null,
          "description": ""
        },
        "swift-coverage.coveredGutterIcon.light": {
          "type": "string",
          "default": null,
          "description": ""
        },
        "swift-coverage.notCoveredGutterIcon.dark": {
          "type": "string",
          "default": null,
          "description": ""
        },
        "swift-coverage.notCoveredGutterIcon.light": {
          "type": "string",
          "default": null,
          "description": ""
        },
        "swift-coverage.mixedGutterIcon.dark": {
          "type": "string",
          "default": null,
          "description": ""
        },
        "swift-coverage.mixedGutterIcon.light": {
          "type": "string",
          "default": null,
          "description": ""
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "lint": "eslint src --ext ts",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.48.0",
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.0",
    "@types/node": "^14.0.27",
    "eslint": "^7.6.0",
    "@typescript-eslint/eslint-plugin": "^3.8.0",
    "@typescript-eslint/parser": "^3.8.0",
    "glob": "^7.1.6",
    "mocha": "^8.0.1",
    "typescript": "^3.8.3",
    "vscode-test": "^1.4.0"
  }
}
